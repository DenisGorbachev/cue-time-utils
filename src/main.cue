package time_utils

import (
	"time"
)

#to_timestamp: {
	input:  time.Time
	parts:  time.Split(input)
	output: parts.second + parts.minute*60 + parts.hour*60*60 + (parts.day-1)*60*60*24 + #month_offsets[parts.month-1] + #year_offsets[parts.year-1970]
}

